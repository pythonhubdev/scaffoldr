{
	"include": [
		"src",
		"tests"
	],
	"exclude": [
		".venv",
		"**/__pycache__",
		"dist",
		"build",
		".git",
		"src/scaffoldr/templates"
	],
	"defineConstant": {
		"DEBUG": true
	},
	"stubPath": "",
	"reportMissingImports": "error",
	"reportMissingTypeStubs": false,
	"pythonVersion": "3.13",
	"pythonPlatform": "All",
	"useLibraryCodeForTypes": true,
	"autoImportCompletions": true,
	"reportUnusedCallResult": false,
	"reportImplicitStringConcatenation": false
}
